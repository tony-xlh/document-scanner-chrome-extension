{
  "name": "Document Scanner",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["Resources/*"],
      "matches": ["<all_urls>"]
    }
  ]
}